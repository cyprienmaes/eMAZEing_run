                                 ; -------------------------------------------------------- PRESS START ------------------------------------------------------------------
                                 PRESSstart :
                                 /*
                                  * Y pointer register is used to write 'PRESS START' on the screen. 
                                  * The characters are defined in the flash meory in the CharTable at the botton of the code.
                                  */
000021 e0c0                      	LDI YL,0x00
000022 e0d1                      	LDI YH,0x01
000023 e000                      	LDI R16,0b00
000024 e100                      	LDI R16,16         ;P
000025 9309                      	ST Y+,R16		  
000026 e101                      	LDI R16,17		  ;R
000027 9309                      	ST Y+,R16
000028 e00c                      	LDI R16,12		  ;E
000029 9309                      	ST Y+,R16
00002a e102                      	LDI R16,18		  ;S
00002b 9309                      	ST Y+,R16
00002c 9309                      	ST Y+,R16         ;S
00002d e00a                      	LDI R16,10
00002e e016                      	LDI R17,6
                                 	loop :
00002f 9309                      		ST Y+,R16
000030 951a                      		DEC R17
000031 f7e9                      	BRNE loop
000032 e102                      	LDI R16,18         ;S
000033 9309                      	ST Y+,R16
000034 e103                      	LDI R16,19         ;T
000035 9309                      	ST Y+,R16
000036 e00b                      	LDI R16,11         ;A
000037 9309                      	ST Y+,R16
000038 e101                      	LDI R16,17         ;R
000039 9309                      	ST Y+,R16
00003a e103                      	LDI R16,19         ;T
00003b 8308                      	ST Y,R16
                                 
                                 init :
                                 /*
                                  * Initializing the screen outputs (shift registers) and setting up the counter for the random number. 
                                  */
                                 
                                 	.DEF random = R23
00003c ec68                      	LDI R22,200				; Counter for the flashing PRESS START
00003d ef7f                      	LDI random,255
00003e 9468                      	SET						; Set T in SREG.
                                    
                                 	; Setting up the SDI (PB3).
00003f 9a23                      	SBI DDRB,3			    ; Pin PB3 is an output
000040 982b                      	CBI PORTB,3	
                                 	
                                 	; Setting up the data latch (LE/DM1)
000041 9a24                      	SBI DDRB,4				; Pin PB4 is an output
000042 982c                      	CBI PORTB,4
                                 	
                                 	; Enabling the shift latch clock.
000043 9a25                      	SBI DDRB,5				; Pin PB5 is an output
000044 982d                      	CBI PORTB,5
                                 
000045 c015                      	RJMP start
                                 
                                 ; ------------------- shift of the row in the screen ------------------------
                                 rowon :
000046 1f55                      	ROL R21				; Rotate to the left
000047 f408                      	BRCC rowoff			; Branch if Carry Cleared
000048 9a2b                      	SBI PORTB,3
                                 rowoff :
000049 9a2d                      	SBI PORTB,5
00004a 982d                      	CBI PORTB,5
00004b 982b                      	CBI PORTB,3
00004c 951a                      	DEC R17           ; Do this 8 times
00004d f7c1                      	BRNE rowon
                                 
00004e 9a2c                      SBI PORTB,4           ; PB4 need to stay at one a certain amount of time
00004f ef3f                      LDI R19,255
000050 e049                      LDI R20, 9
                                 wait1:
                                 	wait2:
000051 954a                      		DEC R20
000052 f7f1                      		BRNE wait2
000053 e049                      	LDI R20,9
000054 953a                      	DEC R19
000055 f7d9                      	BRNE wait1
000056 982c                      CBI PORTB,4
                                 
000057 950a                      DEC R16                ; Change the position of the byte in flash memory
000058 9526                      LSR R18                ; shift to the rigth the shift register
000059 f009                      BREQ start             ; if shift equal to 0 => go to start
00005a c00a                      RJMP display           ; go to display either
                                 ; --------------------------------------------------------------------------
                                 
                                 start :
00005b e006                      	LDI R16,6               ; Count from 6 to 0 to have all the flash line
00005c e420                      	LDI R18, 0b01000000     ; Send the row
00005d 956a                      	DEC R22
00005e f431                      	BRNE display
00005f f01e                      	BRTS clearT				; Branch if T Flag Set.
000060 ec68                      	LDI R22,200
000061 9468                      	SET
000062 c002                      	RJMP display
                                 	clearT :
000063 e169                      		LDI R22,25
000064 94e8                      		CLT
                                 
                                 display :
000065 e1c0                      	LDI YL,0x10
000066 e0d1                      	LDI YH,0x01             ; 16 blocks to display
000067 e018                      	LDI R17,8               ; constant
000068 e130                      	LDI R19,16              ; counter of each blocks
                                 
000069 ef5f                      	LDI R21,0xFF            ; Transition on PIN D (intermediate state)
00006a b95a                      	OUT DDRD,R21
00006b b95b                      	OUT PORTD,R21
                                 
                                 	; Configure the PIN D of the keyboard
00006c ef50                      	LDI R21,0xF0   
00006d b95a                      	OUT DDRD,R21            ; PIN 7:4 are outputs and 3:0 are inputs
00006e e05f                      	LDI R21,0x0F
00006f b95b                      	OUT PORTD,R21			; PIN 3:0 have pull-up resistor on
000070 c001                      	RJMP COUNTloop
                                 
                                 NEWcounter :
000071 ef7f                      	LDI random,255
                                 
                                 COUNTloop :
000072 ef5f                      	LDI R21,0xFF            ; Transition on PIN D (intermediate state)
000073 b95a                      	OUT DDRD,R21
000074 b95b                      	OUT PORTD,R21
                                 
000075 ef50                      	LDI R21,0xF0   
000076 b95a                      	OUT DDRD,R21            ; PIN 7:4 are outputs and 3:0 are inputs
000077 e05f                      	LDI R21,0x0F
000078 b95b                      	OUT PORTD,R21			; PIN 3:0 have pull-up resistor on
                                 
000079 957a                      	DEC random              ; Decrement random
00007a 3071                      	CPI random,1		    ; IF random equal 1
00007b f3a9                      	BREQ NEWcounter
00007c 9b48                      	SBIS PIND,0				; Button is pushed in the column 0 go to the next instruction
00007d c001                      	RJMP READloop
00007e c00a                      	RJMP blocksloop
                                 
                                 READloop :
00007f ef5f                      	LDI R21,0xFF            ; Transition on PIN D
000080 b95a                      	OUT DDRD,R21
000081 b95b                      	OUT PORTD,R21
                                 
000082 e05f                      	LDI R21,0x0F			; PIN 7:4 are inputs and 3:0 are outputs
000083 b95a                      	OUT DDRD,R21
000084 ef50                      	LDI R21,0xF0			; PIN 7:4 have pull-up resistor on
000085 b95b                      	OUT PORTD,R21
000086 0000                      	NOP
                                 
000087 9b4c                      	SBIS PIND,4				; Button is pushed in the column 0 and row 4 go to the next instruction
000088 c01b                      	RJMP begin
                                 
                                 blocksloop :
000089 e4e8                      	LDI ZL,low(CharTable << 1)			; First element in flash memory
00008a e0f5                      	LDI ZH,high(CharTable << 1)
00008b f016                      	BRTS show							; Branch if T Flag Set.
                                 
                                 notshow :
00008c e050                      	LDI R21,0
00008d c00a                      	RJMP block
                                 
                                 show :
                                 /*
                                  * Z ponints toward the chartable and Y points toward the index of the chosen symbol in the chartable.
                                  * This branch is responsible for showing the chosen symbols from the CharTable.
                                  */
00008e 915a                      	LD R21,-Y                       ; Load the line that we need on the flash (Load Indirect and Pre-Dec.) Last is sent first.
00008f 9f51                      	MUL R21,R17                     ; multiply by eight to have the first element of the line R1:R0 <- Rd x Rr
000090 2d50                      	MOV R21,R0						; Move Between Registers Rd <- Rr
000091 0f50                      	ADD R21,R16					    ; Add row to have the byte of the line
000092 1fe5                      	ADC ZL,R21                      ; Add this value on Z
000093 f410                      	BRCC nc							; Branch if Carry Cleared
000094 e051                      	LDI R21,1
000095 0ff5                      	ADD ZH,R21
                                 	nc :
000096 9154                      	LPM R21,Z		                ; Load the byte at the position stored in Z in R21
000097 e045                      	LDI R20,5                       ; We must shift 5 times
                                 
                                 block :
000098 9557                      	ROR R21           ; shift to the right R21 
000099 f408                      	BRCC turnoff   
00009a 9a2b                      	SBI PORTB,3		  ; If it's a one turn on the LED
                                 
                                 turnoff :
00009b 9a2d                      	SBI PORTB,5       ; Rising edge to put in the shift register
00009c 982d                      	CBI PORTB,5
00009d 982b                      	CBI PORTB,3       ; Don't forget to clear PB3
00009e 954a                      	DEC R20           ; Decrement counter2 5 times
00009f f7c1                      	BRNE block
                                 
0000a0 953a                      DEC R19               ; Decrement counter1 16 times (corresponding to the number of block)
0000a1 f739                      BRNE blocksloop
0000a2 2f52                      MOV R21,R18           ; Put the row shifting on another register
0000a3 cfa2                      RJMP rowon
                                 
