00010e f7e9                      	BRNE WriteMazeToScreenbuffer9
                                 
                                 
                                 ;Send data to screenbuffer------------------------------------------------------------------------------
00010f e0e0                      LDI ZL,0x00						; ZL is the register R30------Z = ZL+ZH
000110 e0f1                      LDI ZH,0x01						; init Z to point do address 0x0100----------ZH is the register R31
000111 e000                      LDI R16 ,0x00					; We will write this value to every byte of the whole screenbuffer
                                 
000112 e436                      LDI R19,70						; Need to write 70 bytes to fill the whole screenbuffer
                                 WriteByteToScreenbuffer:
000113 9301                      	ST Z+,R16					;write value from Ra to address pointed by Z and auto-increase Z pointer
000114 953a                      	DEC R19
000115 f7e9                      	BRNE WriteByteToScreenbuffer	;write 70 bytes
                                 ;-------------------------------------------------------------------------------------------------------
                                 
                                 ;Send obstacle to screenbuffer------------------------------------------------------------------------------
000116 e0c0                      LDI YL,0x00						; YL is the register R32------Y = YL+YH
000117 e0d2                      LDI YH,0x02						; init Y to point do address 0x0200----------YH is the register R33
                                 
000118 e436                      LDI R19,70						;need to write 70 bytes to fill the whole screenbuffer
                                 WriteObstacleToScreenbuffer:
000119 9309                      	ST Y+,R16						;write value from Ra to address pointed by Y and auto-increase Y pointer
00011a 953a                      	DEC R19
00011b f7e9                      	BRNE WriteObstacleToScreenbuffer	;write 70 bytes
                                 ;-------------------------------------------------------------------------------------------------------
                                 
                                 ; Add the move point on the screen at a deterministic place
00011c e0e0                      LDI ZL,0					    ; Take the position of the byte
00011d e011                      LDI bytesnake, 0x01                   ; Set one bit on the byte
00011e 8310                      ST Z,bytesnake                        ; Put the value pointed by Z
                                 
                                 ;Add food for snake on the screen
                                 generate :
00011f 7f7f                      	ANDI random,0b011111111         ; We want a random number < 70 => we need 7 bits => random,7 = 0
000120 3476                      	CPI random,70                   ; Compare random register to 70
000121 f408                      	BRSH LSFR270                    ; If it's equal or bigger than 70 use LSFR
000122 c00a                      	RJMP food                       
                                 LSFR270 :
000123 2f07                      	MOV R16, random                 ; Clone random to R16                    ex : R16 = 0b01111110
000124 2f37                      	MOV R19, random                 ; Clone random to R19                         R17 = 0b01111110
000125 9576                      	LSR random                      ; Shift to the right random                   random = 0b00111111
000126 fb00                      	BST R16,0					    ; Take the first (LSB) bit of R16                   T = 0
000127 f976                      	BLD random,6                    ; Put this at seventh place of random         random = 0b00111111
000128 f936                      	BLD R19,6                       ; Same for R19                                R17 = 0b00111110
000129 2730                      	EOR R19,R16                     ; R19 = R16 xor R19                           R17 = 0b01000000
00012a fb36                      	BST R19,6                       ; Take the seventh bit of R19                 T = 1
00012b f975                      	BLD random,5                    ; Put this at the sixth place of random       random = 0b00111111
00012c cff2                      	RJMP generate                   ; Test if the new random is smaller than 70
                                 
                                 food :
00012d e820                      	LDI bytefood, 0x80                   ; Set one bit on the byte
00012e e0a0                      	LDI XL,0x00
00012f 0fa7                      	ADD XL,random
000130 919c                      	LD R25,X
000131 2392                      	AND R25,bytefood				; Checkin if the generated position of the fruit is free.
000132 3090                      	CPI R25,0						; Compare Register with Immediate
000133 f779                      	BRNE LSFR270
000134 e0c0                      	LDI YL,0x00                     ; Begin Y = 0x0200
000135 0fc7                      	ADD YL,random				    ; Add it the random number smaller than 70
000136 8328                      	ST Y,bytefood			      	    ; Put the value pointed by Y
                                 
000137 e096                      LDI direction,6						;Initial direction (right) of the snake
                                 
                                 
                                 InitKeyboard:
                                     ; Configure input pin PD0 
000138 ef0f                      	LDI R16,0xFF
000139 b90a                      	OUT DDRD,R16
00013a b90b                      	OUT PORTD,R16               ; Transition of PIND (intermediate state)
                                 
00013b ef00                      	LDI R16,0xF0		        ; PIND 7:4 are outputs and 3:0 are inputs
00013c b90a                      	OUT DDRD,R16
00013d e00f                      	LDI R16,0x0F				; PIND 3:0 have pull-up resistor
00013e b90b                      	OUT PORTD,R16
                                 	
                                 Main:
                                 /*
                                  * The Main is responsible for reading user's inputs on the keyboard.
                                  */
