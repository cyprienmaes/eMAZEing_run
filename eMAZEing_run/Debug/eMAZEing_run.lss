                                 ; -------------------------------------------------------- GAME ------------------------------------------------------------------------
                                 
                                 begin:
                                 	.DEF direction	= R25
                                 	.DEF bytesnake	= R17
                                 	.DEF bytefood	= R18
                                 	.DEF speed		= R22
                                 	.DEF toggle		= R24			; Used to make the fruit darker
                                 	.DEF score1		= R8
                                 	.DEF score2     = R9
0000a4 e06f                      	LDI speed,0x0F
0000a5 e000                      	LDI R16,0
0000a6 2e80                      	MOV score1,R16
0000a7 2e90                      	MOV score2, R16
0000a8 e081                      	LDI toggle, 1
                                 	;-------------------TIMERS----------------------------------
                                 
                                 	;Set timer0 prescaler to 256
0000a9 e004                      	LDI	R16,4
0000aa bd05                      	OUT TCCR0B,R16	
                                 
                                 	; The PRTIM1 bit in PRR  Power Reduction Register must be written to zero to enable Timer/Counter1 module.
0000ab e000                      	LDI R16,0
0000ac 9300 0064                 	STS PRR,R16
                                 	;Set timer1 prescaler to 1024
0000ae e003                      	LDI	R16,3
0000af 9300 0081                 	STS TCCR1B,R16	
                                 
                                 	;Setting the TCNT0 value at 312Hz
0000b1 e308                      	LDI R16,56	
0000b2 bd06                      	OUT TCNT0,R16
                                 
                                 	;Setting the initial TCNT1 value.
0000b3 ef0f                      	LDI R16,0xFF	
0000b4 2f36                      	MOV R19,speed
0000b5 9330 0085                 	STS TCNT1H,R19
0000b7 9300 0084                 	STS TCNT1L,R16
                                 
                                 	;enable global interrupt & timer0 and timer1 interrupt
0000b9 e800                      	LDI	R16,0x80
0000ba bf0f                      	OUT	SREG,R16
0000bb e001                      	LDI R16,1
0000bc 9300 006e                 	STS	TIMSK0,R16
0000be 9300 006f                 	STS TIMSK1,R16
                                 
                                 	;Clearing the outputs
0000c0 9a23                      	SBI DDRB,3			    ; Pin PB3 is an output
0000c1 982b                      	CBI PORTB,3	
                                 
0000c2 9a24                      	SBI DDRB,4				; Pin PB4 is an output
0000c3 982c                      	CBI PORTB,4
                                 
0000c4 9a25                      	SBI DDRB,5				; Pin PB5 is an output
0000c5 982d                      	CBI PORTB,5
                                 
                                 ;Send maze to screenbuffer------------------------------------------------------------------------------
0000c6 e0a0                      LDI XL,0x00						
0000c7 e0b3                      LDI XH,0x03						
0000c8 e000                      LDI R16 ,0x00					
0000c9 e037                      LDI R19,7						
                                 WriteMazeToScreenbuffer:
0000ca 930d                      	ST X+,R16					
0000cb 953a                      	DEC R19
0000cc f7e9                      	BRNE WriteMazeToScreenbuffer	
                                 
                                 
0000cd e100                      LDI R16 ,0x10					
0000ce 930d                      ST X+,R16
                                 
                                 
0000cf e000                      LDI R16 ,0x00					
0000d0 e038                      LDI R19,8					
                                 WriteMazeToScreenbuffer2:		
0000d1 930d                      	ST X+,R16		
0000d2 953a                      	DEC R19
0000d3 f7e9                      	BRNE WriteMazeToScreenbuffer2	
                                 
0000d4 ef0e                      LDI R16 ,0xFE					
0000d5 930d                      ST X+,R16	
                                 
0000d6 e10f                      LDI R16 ,0x1F					
0000d7 930d                      ST X+,R16	
                                 
0000d8 e000                      LDI R16 ,0x00					
0000d9 e038                      LDI R19,8					
                                 WriteMazeToScreenbuffer3:		
0000da 930d                      	ST X+,R16		
0000db 953a                      	DEC R19
0000dc f7e9                      	BRNE WriteMazeToScreenbuffer3
                                 
0000dd e002                      LDI R16 ,0x02					
0000de 930d                      ST X+,R16
                                 
0000df e100                      LDI R16 ,0x10					
0000e0 930d                      ST X+,R16
                                 
0000e1 e000                      LDI R16 ,0x00					
0000e2 e034                      LDI R19,4					
                                 WriteMazeToScreenbuffer4:		
0000e3 930d                      	ST X+,R16		
0000e4 953a                      	DEC R19
0000e5 f7e9                      	BRNE WriteMazeToScreenbuffer4
                                 
0000e6 ef00                      LDI R16 ,0xF0					
0000e7 930d                      ST X+,R16
                                 
0000e8 ef0f                      LDI R16 ,0xFF					
0000e9 e033                      LDI R19,3					
                                 WriteMazeToScreenbuffer5:		
0000ea 930d                      	ST X+,R16		
0000eb 953a                      	DEC R19
0000ec f7e9                      	BRNE WriteMazeToScreenbuffer5
                                 
0000ed e003                      LDI R16 ,0x03					
0000ee 930d                      ST X+,R16
                                 
0000ef ef00                      LDI R16 ,0xF0					
0000f0 930d                      ST X+,R16
                                 
0000f1 ef0f                      LDI R16 ,0xFF					
0000f2 e032                      LDI R19,2					
                                 WriteMazeToScreenbuffer6:		
