0000f5 f7e9                      	BRNE WriteMazeToScreenbuffer6
                                 
0000f6 ef0f                      LDI R16 ,0xFF					
0000f7 930d                      ST X+,R16
                                 
0000f8 e70f                      LDI R16 ,0x7F					
0000f9 930d                      ST X+,R16
                                 
0000fa e100                      LDI R16 ,0x10					
0000fb 930d                      ST X+,R16
                                 
0000fc e000                      LDI R16 ,0x00					
0000fd e039                      LDI R19,9					
                                 WriteMazeToScreenbuffer7:		
0000fe 930d                      	ST X+,R16		
0000ff 953a                      	DEC R19
000100 f7e9                      	BRNE WriteMazeToScreenbuffer7
                                 
000101 e100                      LDI R16 ,0x10					
